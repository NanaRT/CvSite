        {% block menu %}{% endblock %}
        {% block body %}{% endblock %}
        
		<div class="footer">
			<div class="col-md-3">
			</div>
			<div class="col-md-2">
				<img src="{{ asset('bundles/cvsite/images/my_logo.png') }}"/>
			</div>
			<div class="col-md-2">
				 Alana TAYLOR </br>
				 Bordeaux, FRANCE 
			</div>
			<div class="col-md-2">
				+33 6 68 21 24 42</br>
				alanataylor@hotmail.fr
			</div>
		</div>
